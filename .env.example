# Jira API Configuration
# Copy this file to .env and fill in your actual values

# Your Jira instance URL (without trailing slash)
# Example: https://your-domain.atlassian.net
REACT_APP_JIRA_BASE_URL=

# Your Jira email address
REACT_APP_JIRA_EMAIL=

# Your Jira API token (generate from: https://id.atlassian.com/manage-profile/security/api-tokens)
REACT_APP_JIRA_API_TOKEN=

# Optional: Default project key to filter by (leave empty for all projects)
REACT_APP_DEFAULT_PROJECT=
